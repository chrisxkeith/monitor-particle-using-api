<!-- This may or may not be up-to-date. Caveat emptor. -->
<doc>
    <sheets>
         <sheet>
            <!-- From the Google sheet URL. -->
            <sheetId>fu</sheetId>
            <!-- How often the server will update the sheet. Also data granularity. -->
            <writeIntervalInSeconds>60</writeIntervalInSeconds>
            <!-- The full time data range of the sheet. -->
            <dataIntervalInMinutes>3600</dataIntervalInMinutes>
            <htmlWriteIntervalInSeconds>5</htmlWriteIntervalInSeconds>
            <dataSet>
                <microcontrollerName>photon-09</microcontrollerName>
                <sensors>
                    <sensor>
                        <!-- Name as reported by the microcontroller. -->
                        <name>unweighted</name>
                        <!-- Name as displayed in the column header in the Google Sheet. -->
                        <displayName>Clothes Dryer</displayName>
                    </sensor>
                </sensors>
            </dataSet>
        </sheet>
    </sheets>
</doc>
